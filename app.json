{
  "expo": {
    "name": "SurvivorFitnessApp",
    "slug": "SurvivorFitnessApp",
    "version": "0.2.0",
    "orientation": "portrait",
    "icon": "./assets/home-screen-icon.png",
    "splash": {
      "image": "./assets/SF-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "buildNumber": "0.1.0",
      "bundleIdentifier": "com.survivorfitness.change.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
